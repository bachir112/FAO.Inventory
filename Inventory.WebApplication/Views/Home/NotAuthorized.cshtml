
@{
    ViewBag.Title = "NotAuthorized";
}

<style type="text/css">

    .theme-panel.hidden-xs.hidden-sm{
        display:none;
    }

</style>

<div class="page-content-wrapper">
    <div class="page-content">

        @Html.Partial("_ThemeColors")

        <div class="row">
            <div class="col-lg-12 col-xs-12 col-sm-12">
                <div class="portlet light bordered">
                    <div class="portlet-title" style="text-align:center;">
                        <div class="caption" style="float:none;">
                            <i class="icon-bubble font-dark hide"></i>
                            <span class="caption-subject font-hide bold uppercase">Unauthorized Access</span>
                        </div>
                    </div>
                    <div class="portlet-body">
                        <div class="row">
                            <div class="col-md-12">
                                <div class="mt-widget-1 addNewItemsSection">
                                    <div class="mt-img">
                                        <img src="../images/unauthorized.png">
                                    </div>
                                    <div class="mt-body">
                                        <h3 class="mt-username">
                                            <a style="text-decoration:none;" href="@Url.Action("Index","Home")">Take me back</a>
                                        </h3>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>